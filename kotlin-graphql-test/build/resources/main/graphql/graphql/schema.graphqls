type DefaultListResponse {
    statusCode: Int!,
    message: String!,
    data: [String!]!,
    pagination: String
}

type DefaultResponse {
    statusCode: Int!,
    message: String!,
    data: String
}

type Pagination {
    perPageSize: Int!,
    totalElementCount: Int!,
    totalPage: Int!,
    orderBy: Boolean
}

input PageRequestDto {
    currentPage: Int,
    perPageSize: Int,
    orderBy: Boolean
}