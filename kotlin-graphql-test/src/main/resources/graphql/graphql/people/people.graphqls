input PeopleCreateRequestDto {
    teamId: ID!,
    lastName: String!,
    firstName: String!,
    sex: String!,
    bloodType: String!,
    serveYears: Int!,
    role: String!,
    hometown: String!
}

input PeopleUpdateRequestDto {
    teamId: ID,
    lastName: String,
    firstName: String,
    sex: String,
    bloodType: String,
    serveYears: Int,
    role: String,
    hometown: String
}

input PeopleSearchRequestDto {
    lastName: String,
    firstName: String,
    sex: String,
    bloodType: String,
    serveYears: Int,
    role: String,
    hometown: String
}

type DefaultListPeopleResponse {
    statusCode: Int!,
    message: String!,
    data: [PeopleResponseDto!]!,
    pagination: Pagination
}

type DefaultPeopleResponse {
    statusCode: Int!,
    message: String!,
    data: PeopleResponseDto!
}

type PeopleResponseDto {
    peopleId: ID!,
    teamId: ID!,
    lastName: String!,
    firstName: String!,
    sex: String!,
    bloodType: String!,
    serveYears: Int!,
    role: String!,
    hometown: String!
}

