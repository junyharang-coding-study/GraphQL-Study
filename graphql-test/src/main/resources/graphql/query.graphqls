    getSupplyBySupplyId(supplyId: String!): DefaultResponse!

    getSupplyByTeamId(
        teamId: Int!,
        page: Int,
        size: Int): DefaultListResponse!

    getTeamList(
        manager: String,
        office: String,
        extensionNumber: String,
        mascot: String,
        cleaningDuty: String,
        project: String,
        page: Int,
        size: Int): DefaultListResponse!,

