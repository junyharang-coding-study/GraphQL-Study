type Mutation {
    saveEquipment(input: EquipmentCreateRequestDto): DefaultEquipmentResponse!
    updateEquipment(
        equipmentId: String,
        input: EquipmentRequestDto
    ): DefaultEquipmentResponse!
    deleteEquipment(equipmentId: String!): DefaultEquipmentResponse!

    saveForPeople(input: PeopleRequestDto): DefaultResponse!
    updatePeople(input: PeopleUpdateRequestDto): DefaultResponse!
    deletePeople(peopleId: Int!): DefaultResponse!

    saveForSoftware(input: SoftwareRequestDto): DefaultResponse!
    updateSoftware(input: SoftwareUpdateRequestDto): DefaultResponse!
    deleteSoftware(softwareId: String!): DefaultResponse!

    saveForSupply(input: SupplyRequestDto): DefaultResponse!
    deleteSupply(supplyId: String!): DefaultResponse!

    saveTeam(input: TeamCreateRequestDto): DefaultTeamResponse!
    updateTeam(
        teamId: ID,
        input: TeamUpdateRequestDto): DefaultTeamResponse!
    deleteTeam(teamId: ID!): DefaultTeamResponse!

    saveForRole(input: RoleRequestDto): DefaultResponse!
    updateRole(input: RoleUpdateRequestDto): DefaultResponse!
    deleteRole(roleId: String!): DefaultResponse!
}